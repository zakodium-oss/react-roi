{
  "name": "react-roi",
  "version": "1.5.0",
  "files": [
    "lib-esm",
    "src"
  ],
  "type": "module",
  "exports": {
    ".": "./lib-esm/index.js"
  },
  "scripts": {
    "build": "storybook build",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib-esm",
    "dev": "storybook dev -p 6006",
    "eslint": "eslint ./ --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "playwright": "npm run playwright-ct -- --project chromium",
    "playwright-ct": "playwright test -c playwright-ct.config.ts",
    "prepack": "npm run tsc",
    "prettier": "prettier --check ./",
    "prettier-write": "prettier --write ./",
    "test": "npm run playwright && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "vitest",
    "tsc": "npm run clean && npm run tsc-esm",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "dependencies": {
    "immer": "^10.2.0"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.56.1",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-docs": "^10.0.2",
    "@storybook/react-vite": "^10.0.2",
    "@types/node": "^24.9.2",
    "@types/react": "^18.3.26",
    "@zakodium/tsconfig": "^1.0.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-config-zakodium": "^17.0.0",
    "image-js": "^1.0.0",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-kbs": "^2.1.1",
    "rimraf": "^6.1.0",
    "storybook": "^10.0.2",
    "typescript": "~5.9.3",
    "uuid": "^13.0.0",
    "vite": "^7.1.12",
    "vitest": "^4.0.5"
  }
}
